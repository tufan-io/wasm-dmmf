{
  "name": "@tufan-io/wasm-dmmf",
  "version": "4.5.0",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
    "build": "rm -rf pkg && rm -rf dist && npm run wasm && npm run ts && npm run mv",
    "mv": "rm -rf dist/pkg && mkdir dist/pkg && cp pkg/wasm_dmmf* dist/pkg",
    "ts": "tsc index.ts --target es6 --outDir dist --declaration --moduleResolution node",
    "wasm": "wasm-pack build --release --target web --scope tufan-io"
  },
  "keywords": [],
  "author": "Jayden",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^18.11.15",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@prisma/generator-helper": "^4.5.0"
  }
}
